        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#CEE2FF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "9748cb57-8c12-4a55-baa8-85b16595cbde"
      }
    }
  }
